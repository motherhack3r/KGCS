@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sec: <https://example.org/sec/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://example.org/engagement/ENGAGE-EAC0001> a sec:EngagementConcept ;
    rdfs:label "API Monitoring"^^xsd:string ;
    sec:description "Monitor local APIs that might be used by adversary tools and activity."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0001"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0002> a sec:EngagementConcept ;
    rdfs:label "Network Monitoring"^^xsd:string ;
    sec:description "Monitor network traffic in order to detect adversary activity."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0002"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0003> a sec:EngagementConcept ;
    rdfs:label "System Activity Monitoring"^^xsd:string ;
    sec:description "Collect system activity logs that can reveal adversary activity."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0003"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0004> a sec:EngagementConcept ;
    rdfs:label "Network Analysis"^^xsd:string ;
    sec:description "Analyze network traffic to gain intelligence on communications between systems."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0004"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0005> a sec:EngagementConcept ;
    rdfs:label "Lures"^^xsd:string ;
    sec:description "Deceptive systems and artifacts intended to serve as decoys, breadcrumbs, or bait to elicit a specific response from the adversary."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0005"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0006> a sec:EngagementConcept ;
    rdfs:label "Application Diversity"^^xsd:string ;
    sec:description "Present the adversary with a variety of installed applications and services."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0006"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0007> a sec:EngagementConcept ;
    rdfs:label "Network Diversity"^^xsd:string ;
    sec:description "Use a diverse set of devices on the network to help establish the legitimacy of a deceptive network."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0007"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0008> a sec:EngagementConcept ;
    rdfs:label "Burn-In"^^xsd:string ;
    sec:description "Exercise a target system in a manner where it will generate desirable system artifacts."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0008"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0009> a sec:EngagementConcept ;
    rdfs:label "Email Manipulation"^^xsd:string ;
    sec:description "Modify the flow of email in the environment."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0009"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0010> a sec:EngagementConcept ;
    rdfs:label "Peripheral Management"^^xsd:string ;
    sec:description "Manage peripheral devices used on systems within the network for engagement purposes."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0010"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0011> a sec:EngagementConcept ;
    rdfs:label "Pocket Litter"^^xsd:string ;
    sec:description "Data used to support the engagement narrative."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0011"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0012> a sec:EngagementConcept ;
    rdfs:label "Personas"^^xsd:string ;
    sec:description "Create fictitious human user(s) through a combination of planted data and revealed behavior patterns."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0012"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0013> a sec:EngagementConcept ;
    rdfs:label "Malware Detonation"^^xsd:string ;
    sec:description "Execute malware under controlled conditions to analyze its functionality."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0013"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0014> a sec:EngagementConcept ;
    rdfs:label "Software Manipulation"^^xsd:string ;
    sec:description "Make changes to a system’s software properties and functions to achieve a desired effect."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0014"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0015> a sec:EngagementConcept ;
    rdfs:label "Information Manipulation"^^xsd:string ;
    sec:description "Conceal and reveal both facts and fictions to support a deception story"^^xsd:string ;
    sec:engageId "ENGAGE-EAC0015"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0016> a sec:EngagementConcept ;
    rdfs:label "Network Manipulation"^^xsd:string ;
    sec:description "Make changes to network properties and functions to achieve a desired effect."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0016"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0017> a sec:EngagementConcept ;
    rdfs:label "Hardware Manipulation"^^xsd:string ;
    sec:description "Alter the hardware configuration of a system to limit what an adversary can do with the device."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0017"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0018> a sec:EngagementConcept ;
    rdfs:label "Security Controls"^^xsd:string ;
    sec:description "Alter security controls to make the system more or less vulnerable to attack."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0018"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0019> a sec:EngagementConcept ;
    rdfs:label "Baseline"^^xsd:string ;
    sec:description "Identify key system elements to establish a baseline and be prepared to reset a system to that baseline when necessary."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0019"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0020> a sec:EngagementConcept ;
    rdfs:label "Isolation"^^xsd:string ;
    sec:description "Configure devices, systems, networks, etc. to contain activity and data, thus preventing the expansion of an engagement beyond desired limits."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0020"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0021> a sec:EngagementConcept ;
    rdfs:label "Attack Vector Migration"^^xsd:string ;
    sec:description "Move a malicious link, file, or device from its intended location to an engagement system or network for execution/use."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0021"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0022> a sec:EngagementConcept ;
    rdfs:label "Artifact Diversity"^^xsd:string ;
    sec:description "Present the adversary with a variety of network and system artifacts."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0022"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAC0023> a sec:EngagementConcept ;
    rdfs:label "Introduced Vulnerabilities"^^xsd:string ;
    sec:description "Intentionally introduce vulnerabilities into the environment for the adversary to exploit."^^xsd:string ;
    sec:engageId "ENGAGE-EAC0023"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAP0001> a sec:EngagementConcept ;
    rdfs:label "Collect"^^xsd:string ;
    sec:description "Gather adversary tools, observe tactics, and collect other raw intelligence about the adversary’s activity."^^xsd:string ;
    sec:engageId "ENGAGE-EAP0001"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAP0002> a sec:EngagementConcept ;
    rdfs:label "Detect"^^xsd:string ;
    sec:description "Establish or maintain awareness regarding adversary activity."^^xsd:string ;
    sec:engageId "ENGAGE-EAP0002"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAP0003> a sec:EngagementConcept ;
    rdfs:label "Prevent"^^xsd:string ;
    sec:description "Stop all or part of the adversary’s ability to conduct their operation as intended."^^xsd:string ;
    sec:engageId "ENGAGE-EAP0003"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAP0004> a sec:EngagementConcept ;
    rdfs:label "Direct"^^xsd:string ;
    sec:description "Encourage or discourage the adversary from conducting their operation as intended."^^xsd:string ;
    sec:engageId "ENGAGE-EAP0004"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAP0005> a sec:EngagementConcept ;
    rdfs:label "Disrupt"^^xsd:string ;
    sec:description "Impair an adversary’s ability to conduct their operation as intended."^^xsd:string ;
    sec:engageId "ENGAGE-EAP0005"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAP0006> a sec:EngagementConcept ;
    rdfs:label "Reassure"^^xsd:string ;
    sec:description "Add authenticity to deceptive components to convince an adversary that an environment is real."^^xsd:string ;
    sec:engageId "ENGAGE-EAP0006"^^xsd:string .

<https://example.org/engagement/ENGAGE-EAP0007> a sec:EngagementConcept ;
    rdfs:label "Motivate"^^xsd:string ;
    sec:description "Encourage an adversary to conduct part or all of their mission."^^xsd:string ;
    sec:engageId "ENGAGE-EAP0007"^^xsd:string .

<https://example.org/engagement/ENGAGE-EGO0001> a sec:EngagementConcept ;
    rdfs:label "Expose"^^xsd:string ;
    sec:description "Reveal the presence of ongoing adversary operations."^^xsd:string ;
    sec:engageId "ENGAGE-EGO0001"^^xsd:string .

<https://example.org/engagement/ENGAGE-EGO0002> a sec:EngagementConcept ;
    rdfs:label "Affect"^^xsd:string ;
    sec:description "Negatively impact the adversaries operations."^^xsd:string ;
    sec:engageId "ENGAGE-EGO0002"^^xsd:string .

<https://example.org/engagement/ENGAGE-EGO0003> a sec:EngagementConcept ;
    rdfs:label "Elicit"^^xsd:string ;
    sec:description "Learn about adversaries tactics, techniques, and procedures (TTPs)."^^xsd:string ;
    sec:engageId "ENGAGE-EGO0003"^^xsd:string .

