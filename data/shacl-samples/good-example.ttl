@prefix sec: <https://example.org/sec/core#> .
@prefix inc: <https://example.org/incident#> .
@prefix attck: <https://example.org/sec/attck#> .
@prefix capec: <https://example.org/sec/capec#> .
@prefix cwe: <https://example.org/sec/cwe#> .
@prefix d3fend: <https://example.org/sec/d3fend#> .
@prefix car: <https://example.org/sec/car#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Good example illustrating canonical chain and required IDs

sec:platform-1 a sec:Platform ;
  sec:CPEUri "cpe:2.3:a:example:product:1.2.3" ;
  sec:vendor "example" ;
  sec:product "product" ;
  sec:version "1.2.3" .

sec:config-1 a sec:PlatformConfiguration ;
  sec:matchCriteriaId "cfg-0001" ;
  sec:configurationCriteria "cpe:2.3:a:example:product:1.2.3" ;
  sec:matchesPlatform sec:platform-1 ;
  sec:affected_by sec:vuln-2025-0001 .

sec:vuln-2025-0001 a sec:Vulnerability ;
  sec:cveId "CVE-2025-0001" ;
  sec:description "Example vuln" ;
  sec:scored_by sec:cvss-3.1-1 ;
  sec:caused_by sec:weakness-1 ;
  sec:references sec:ref-1 ;
  sec:affects sec:config-1 .

sec:cvss-3.1-1 a sec:VulnerabilityScore ;
  sec:cvssVersion "3.1" ;
  sec:vectorString "CVSS:3.1/AV:N/AC:L/PR:N/UI:N/S:U/C:H/I:H/A:H" ;
  sec:baseScore "9.8" .

sec:weakness-1 a sec:Weakness ;
  cwe:cwe_id "CWE-79" ;
  rdfs:label "Example weakness" ;
  sec:exploited_by sec:capec-1 .

capec:capec-1 a sec:AttackPattern ;
  capec:capec_id "CAPEC-100" ;
  sec:implemented_as sec:technique-1 .

sec:technique-1 a sec:Technique ;
  attck:technique_id "T9999" ;
  sec:belongs_to sec:tactic-1 ;
  sec:detected_by sec:car-001 ;
  sec:mitigated_by d3fend:d3-001 .

sec:tactic-1 a sec:Tactic ;
  attck:tactic_id "TA0001" .

sec:car-001 a sec:DetectionAnalytic ;
  car:car_id "CAR-0001" .

d3fend:d3-001 a sec:DefensiveTechnique ;
  d3fend:d3fendId "D3-001" .

sec:ref-1 a sec:Reference ;
  sec:referenceUrl "https://example.org/advisory/CVE-2025-0001"^^xsd:anyURI ;
  sec:referenceSource "Vendor Advisory" .
