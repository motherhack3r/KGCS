@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sec: <https://example.org/sec/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://example.org/analytic/CAR-2013-01-002> a sec:DetectionAnalytic ;
    rdfs:label "Autorun Registry Modification"^^xsd:string ;
    sec:carId "CAR-2013-01-002"^^xsd:string ;
    sec:dataSource "Registry Monitoring"^^xsd:string,
        "Windows Event Logs"^^xsd:string ;
    sec:description "Detect modifications to autorun registry keys used for persistence."^^xsd:string ;
    sec:detects <https://example.org/technique/T1547> ;
    sec:logicExpression "Registry(\"HKLM\\\\Software\\\\Microsoft\\\\Windows\\\\CurrentVersion\\\\Run*\") OR Registry(\"HKCU\\\\Software\\\\Microsoft\\\\Windows\\\\CurrentVersion\\\\Run*\")"^^xsd:string .

<https://example.org/analytic/CAR-2013-01-003> a sec:DetectionAnalytic ;
    rdfs:label "Process Termination"^^xsd:string ;
    sec:carId "CAR-2013-01-003"^^xsd:string ;
    sec:dataSource "Event Logs"^^xsd:string,
        "Sysmon"^^xsd:string ;
    sec:description "Detect process termination patterns used to disable security software."^^xsd:string ;
    sec:detects <https://example.org/technique/T1106>,
        <https://example.org/technique/T1562> ;
    sec:logicExpression "ProcessTerminationEvent WHERE (TargetImage LIKE '%\\\\svchost.exe%' OR TargetImage LIKE '%\\\\defender%')"^^xsd:string .

<https://example.org/analytic/CAR-2015-04-001> a sec:DetectionAnalytic ;
    rdfs:label "Curl Activity"^^xsd:string ;
    sec:carId "CAR-2015-04-001"^^xsd:string ;
    sec:dataSource "Network Traffic"^^xsd:string,
        "Process Logs"^^xsd:string ;
    sec:description "Detect curl command usage for data exfiltration or downloading payloads."^^xsd:string ;
    sec:detects <https://example.org/technique/T1041>,
        <https://example.org/technique/T1105> ;
    sec:logicExpression "Process CONTAINS ('curl' OR 'wget') AND CommandLine CONTAINS ('-O' OR '--output')"^^xsd:string .

