@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sec: <https://example.org/sec/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://example.org/deftech/D3FEND-DT-1> a sec:DefensiveTechnique ;
    rdfs:label "Credential Access Protection"^^xsd:string ;
    sec:d3fendId "D3FEND-DT-1"^^xsd:string ;
    sec:description "Implement multi-factor authentication and secure credential storage to prevent credential theft."^^xsd:string ;
    sec:mitigates <https://example.org/technique/T1110>,
        <https://example.org/technique/T1187> ;
    sec:references <https://d3fend.mitre.org> .

<https://example.org/deftech/D3FEND-DT-2> a sec:DefensiveTechnique ;
    rdfs:label "Command Execution Prevention"^^xsd:string ;
    sec:d3fendId "D3FEND-DT-2"^^xsd:string ;
    sec:description "Restrict execution of system commands and script interpreters through application whitelisting and execution policies."^^xsd:string ;
    sec:mitigates <https://example.org/technique/T1059> .

<https://example.org/deftech/D3FEND-DT-3> a sec:DefensiveTechnique ;
    rdfs:label "Input Validation"^^xsd:string ;
    sec:d3fendId "D3FEND-DT-3"^^xsd:string ;
    sec:description "Validate and sanitize all user inputs to prevent injection attacks (SQL, command, code)."^^xsd:string ;
    sec:mitigates <https://example.org/technique/T1059>,
        <https://example.org/technique/T1190> .

