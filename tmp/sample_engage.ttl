@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sec: <https://example.org/sec/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://example.org/engagement/ENGAGE-ENG-1> a sec:EngagementConcept ;
    rdfs:label "Deception by Disinformation"^^xsd:string ;
    sec:category "Strategic"^^xsd:string ;
    sec:description "Provide false information about capabilities, vulnerabilities, or security posture to mislead adversaries about true defensive capabilities."^^xsd:string ;
    sec:disrupts <https://example.org/technique/T1592>,
        <https://example.org/technique/T1598> ;
    sec:engageId "ENGAGE-ENG-1"^^xsd:string ;
    sec:strategicValue "High"^^xsd:string .

<https://example.org/engagement/ENGAGE-ENG-2> a sec:EngagementConcept ;
    rdfs:label "Decoy Network Segments"^^xsd:string ;
    sec:category "Tactical"^^xsd:string ;
    sec:description "Create isolated network segments with decoy systems to contain and study attacker behavior while protecting critical systems."^^xsd:string ;
    sec:disrupts <https://example.org/technique/T1046>,
        <https://example.org/technique/T1057> ;
    sec:engageId "ENGAGE-ENG-2"^^xsd:string ;
    sec:strategicValue "Medium"^^xsd:string .

<https://example.org/engagement/ENGAGE-ENG-3> a sec:EngagementConcept ;
    rdfs:label "Controlled Engagement"^^xsd:string ;
    sec:category "Strategic"^^xsd:string ;
    sec:description "Allow controlled interaction with attacker tools and techniques in a monitored environment to gather intelligence and improve defenses."^^xsd:string ;
    sec:disrupts <https://example.org/technique/T1566>,
        <https://example.org/technique/T1566.002> ;
    sec:engageId "ENGAGE-ENG-3"^^xsd:string ;
    sec:strategicValue "High"^^xsd:string .

